{
  "manifestVersion": "1.0",
  "author_name": "Digi Dev",
  "name": "Chat Survivors Overlay",
  "version": "1.0.0",
  "description": "Overlay leve que mostra o nome do espectador e dados do jogo em tempo real.",
  "support_email": "guigonunes@gmail.com",

  "frontend": {
    "viewer_url": "index.html",
    "can_link_external_content": false,
    "aspect_width": 1920,
    "aspect_height": 1080,
    "scale_behavior": "fit"
  },

  "backend": {
    "can_use_api": false
  },

  "permissions": {
    "bits_enabled": false,
    "subscriptions_enabled": false,
    "chat_enabled": false
  },

  "whitelisted_panel_urls": [],
  "whitelisted_component_urls": [],
  "whitelisted_overlay_urls": [
    "https://chat-survivors-overlay-server.onrender.com",
    "wss://chat-survivors-overlay-server.onrender.com"
  ],

  "allowlisted_urls": [
    "https://chat-survivors-overlay-server.onrender.com",
    "wss://chat-survivors-overlay-server.onrender.com"
  ],

  "allow_vods": false,
  "configurable": false,
  "live_configurable": false
}
